function [] = fgt_predict_parallel(X , xc , A_k , hsigma,e )

ncores = 8;
X_cell = cell(ncores,1);
spacing = floor(size(Y,1) / ncores);
lower=0;
x_all = cell(ncores,1);
for i=1:ncores
    X_cell{i} = X(lower+1:lower+spacing,:);
    lower=lower+spacing;
end
%Kt1 = fgt_predict(X' , xc , A_k , hsigma,e);
parfor i=1:ncores
    y_all{i} = fgt_predict(XT_cell{i}' , xc , A_k , hsigma,e);
end
y = cell2mat(y_all');

end